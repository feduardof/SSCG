[ req ]
default_bits       = 4096
default_md         = sha512
default_keyfile    = /cert/rootCA.key
prompt             = no
encrypt_key        = no
distinguished_name = req_distinguished_name

[ req_distinguished_name ]
countryName            = "BR"
localityName           = "MA"
organizationName       = "Example Corps"
organizationalUnitName = "Example Corps"
commonName             = "*.example.local"
emailAddress           = "example"
